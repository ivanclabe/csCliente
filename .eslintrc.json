{
  "extends": ["react-app", "prettier", "plugin:react/recommended"],
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "no-var": "error",
    "prettier/prettier": "error",
    "react/no-unescaped-entities": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
